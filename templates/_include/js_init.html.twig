{% if not app.request.xmlHttpRequest %}
<script>
(function($){$(document).ready(function() {
    Runalyze.init({{ initOptions|default('') }});
});})(jQuery);

{% if sharedView|default(null) %}Runalyze.Options.setSharedView();{% endif %}
{% if nokia_here_appid|default(null) and nokia_here_token|default(null) %}Runalyze.Options.setNokiaLayerAuth("{{ nokia_here_appid }}", "{{ nokia_here_token }}");{% endif %}
{% if thunderforest_api_key|default(null) %}Runalyze.Options.setThunderforestLayerAuth("{{ thunderforest_api_key }}");{% endif %}
{% if mapbox_api_key|default(null) %}Runalyze.Options.setMapboxLayerAuth("{{ mapbox_api_key }}");{% endif %}
</script>
{% endif %}
