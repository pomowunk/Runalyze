{% extends extends%}

{% block content %}
    <div class="glossary">
        {% block panelHeading %}
        <div class="panel-heading">
            <h1>
                <a {{ urlClass | raw }} href="{{ path('glossary-index') }}">{% trans %}Glossary{% endtrans %}</a>
            </h1>
        </div>
        {% endblock %}



        {% block panelContent %}
        <div class="panel-content">
            <h2>{% block headline %}{% endblock %}</h2>

            {% block glossaryContent %}{% endblock %}
        </div>
        {% endblock %}
        <p class="info">
            This version of Runalyze is not fully translated. Newer features from codeproducer198 are only in german available.
        </p>
    </div>
{% endblock %}
